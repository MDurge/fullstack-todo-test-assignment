{
  "name": "cypress-demo",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "cypress run",
    "test:api": "jest backend/api.test.js",
    "test:unit": "jest",
    "test:all": "npm run test:api && npm run test"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "cypress": "^13.17.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@babel/preset-env": "^7.23.6"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": ["**/*.test.js"]
  }
}
